{
  "name": "@penseapp/discord-notification",
  "version": "0.0.10",
  "description": "Easily send notifications to Discord on Node.js",
  "author": "Joao Mantovani <joao@penseapp.com.br>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penseapp/discord-notification.git"
  },
  "keywords": [
    "discord",
    "notification",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/penseapp/discord-notification/issues"
  },
  "homepage": "https://github.com/penseapp/discord-notification",
  "scripts": {
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --detectOpenHandles --collect-coverage",
    "format": "prettier --config .prettierrc.json --ignore-unknown --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "g": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "files": [
    "dist/**/**"
  ]
}
