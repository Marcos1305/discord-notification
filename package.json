{
  "name": "@penseapp/discord-notification",
  "version": "0.0.1",
  "description": "Easily send notifications to Discord on Node.js",
  "author": "Joao Mantovani <joao@penseapp.com.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penseapp/discord-notification.git"
  },
  "keywords": [
    "discord",
    "notification",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/penseapp/discord-notification/issues"
  },
  "homepage": "https://github.com/penseapp/discord-notification",
  "scripts": {
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --detectOpenHandles --collect-coverage",
    "format": "prettier --config .prettierrc.json --ignore-unknown --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "g": "^2.0.1",
    "tsc": "^1.20150623.0",
    "user": "^0.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "jest": "^27.3.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "files": [
    "dist/**/**"
  ]
}
